{
  "name": "DCD Lab",
  "version": "0.1.5",
  "environment": "development",
  "basehref": "",
  "apiURL": "https://dwd.tudelft.nl/bucket/api",
  "grafanaURL": "https://dwd.tudelft.nl/grafana",
  "authCodeFlow": {
    "issuer": "http://localhost/",
    "redirectUri": "http://localhost",
    "postLogoutRedirectUri": "http://localhost",
    "clientId": "clients:app-dcd",
    "responseType": "code",
    "scope": "offline openid profile email dcd:public dcd:things dcd:properties dcd:types dcd:consents",
    "useSilentRefresh": false,
    "showDebugInformation": true,
    "sessionChecksEnabled": false,
    "timeoutFactor": 0.01,
    "clearHashAfterLogin": false
  }
}